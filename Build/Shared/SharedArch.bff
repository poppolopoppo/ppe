#once

.AllSharedArchs = {}

//------------------------------------------------------------------------------
// Shared X86 Architecture
//------------------------------------------------------------------------------
.SharedArchX86 =
[
    .Arch = 'x86'
    .ArchDefines = { 'ARCH_X86', 'ARCH_32BIT' }
]
.AllSharedArchs = { .SharedArchX86 }

//------------------------------------------------------------------------------
// Shared X64 Architecture
//------------------------------------------------------------------------------
.SharedArchX64 =
[
    .Arch = 'x64'
    .ArchDefines = { 'ARCH_X64', 'ARCH_64BIT' }
]
.AllSharedArchs + { .SharedArchX64 }
