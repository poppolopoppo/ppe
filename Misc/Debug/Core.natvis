  <Type Name="Core::TScalarMatrix&lt;*, *, *&gt;">
    <Expand>
      <ArrayItems>
        <Direction>Forward</Direction>
        <Rank>2</Rank>
        <Size>$T3</Size>
        <ValuePointer>_data.raw</ValuePointer>
      </ArrayItems>
    </Expand>
  </Type>

  <Type Name="Core::TToken&lt;*, char, *, *, * &gt;">
    <DisplayString>{_data.Ptr,[(int)_data.Ptr[-1]]s}</DisplayString>
    <StringView>_data.Ptr,[(int)_data.Ptr[-1]]s</StringView>
    <Expand>
      <Item Name="String">_data.Ptr,[_data.Ptr[-1]]s</Item>
      <Item Name="Length">(int)_data.Ptr[-1]</Item>
    </Expand>
  </Type>
  <Type Name="Core::TToken&lt;*, wchar_t, *, *, * &gt;">
    <DisplayString>{_data.Ptr,[(int)_data.Ptr[-1]]su}</DisplayString>
    <StringView>_data.Ptr,[(int)_data.Ptr[-1]]su</StringView>
    <Expand>
      <Item Name="String">_data.Ptr,[_data.Ptr[-1]]su</Item>
      <Item Name="Length">(int)_data.Ptr[-1]</Item>
    </Expand>
  </Type>

  <Type Name="Core::FFileSystemNode">
    <DisplayString>{_token}</DisplayString>
    <Expand>
      <Item Name="Depth">_depth</Item>
      <LinkedListItems>
        <Size>_depth</Size>
        <HeadPointer>this</HeadPointer>
        <NextPointer>_parent._ptr</NextPointer>
        <ValueNode>*this</ValueNode>
      </LinkedListItems>
    </Expand>
  </Type>

  <Type Name="Core::FBasename">
    <DisplayString>{_basenameNoExt} {_extname}</DisplayString>
  </Type>
  <Type Name="Core::FDirpath">
    <DisplayString Condition="_path._ptr != nullptr">[{_path._ptr->_depth}] {*_path._ptr}"</DisplayString>
    <Expand>
      <Item Name="Depth">_path._ptr->_depth</Item>
      <Item Name="Path" Condition="_path._ptr != nullptr">*_path._ptr</Item>
    </Expand>
  </Type>

  <Type Name="Core::RTTI::TMetaTypeAtom&lt;* &gt;">
    <DisplayString>{_wrapper}</DisplayString>
    <Expand>
      <ExpandedItem>_wrapper</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="Core::TRefPtr&lt;* &gt;">
    <DisplayString>{_ptr}</DisplayString>
    <Expand>
      <Item Name="RefCount" Condition="_ptr != nullptr">_ptr->_refCount</Item>
      <ExpandedItem>_ptr</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="Core::TSafePtr&lt;* &gt;">
    <DisplayString>{_ptr}</DisplayString>
    <Expand>
      <Item Name="RefCount" Condition="_ptr != nullptr">_ptr->_refCount</Item>
      <Item Name="SafeCount" Condition="_ptr != nullptr">_ptr->_safeRefCount</Item>
      <ExpandedItem>_ptr</ExpandedItem>
    </Expand>
  </Type>

</AutoVisualizer>
